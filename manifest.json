{
  "manifest_version": 3,
  "name": "hyperboard",
  "version": "1.0.0",
  "description": "Hyperverge's Own Display board extension",
  "permissions": [
    "activeTab",
    "geolocation"
  ],
  "icons": {
    "32": "images/hyperboard-32.png",
    "64": "images/hyperboard-16.png",
    "128": "images/hyperboard-128.png"
  },

  "chrome_url_overrides": {
    "newtab": "index.html"
  }

}